{
    "name": "md/scope",
    "type": "silverstripe-recipe",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": ">=5.6.0",
        "silverstripe/recipe-plugin": "^1.2",
        "silverstripe/recipe-cms": "4.4.1@stable",
        "md/ac-json-events": "dev-ss4.x-ui",
        "md/saml": "dev-md",
        "md/division-analytics": "1.*",
        "md/uiowa-bar": "1.2.x",
        "md/deploy-helper": "*",
        "gorriecoe/silverstripe-webfonts": "*",
        "jonom/silverstripe-betternavigator": "*",
        "silverstripe/blog": "^3.3",
        "md/silverstripe-maintenance-mode": "dev-master",
        "undefinedoffset/sortablegridfield": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "resources-dir": "_resources",
        "expose": [
            "themes/scope/dist"
        ],
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/division-analytics"
        },
        {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/silverstripe-saml"
        },
        {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/uiowa-bar"
        },
        {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/deploy-helper"
        },
        {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/ac-json-events"
        },
        {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/silverstripe-maintenance-mode"
        }
    ]
}
